<div class="container">
  <div class="row text-center pad-top ">
    <div class="col-md-12">
      <h2><strong>Showing Exist User Data</strong></h2>
    </div>
  </div>
  <div class="row  pad-top">
    <div class="col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3 col-xs-10 col-xs-offset-1">
      <hr />
      <div class="panel panel-default">
        <div class="panel-heading">
          <strong>   User Information </strong>
        </div>
        <div class="panel-body">
          <form ng-app="" action="/user/signup" method="POST" name="signup" role="form">
            <div class="form-group input-group">
              <input type="text" name="studentName" class="form-control" id="studentName" value="<%= student.studentName %>" readonly />
              <span class="input-group-addon"></span>
            </div>
            <div class="form-group input-group">
              <input type="text" name="studentNationalID" class="form-control" id="studentNationalID" value="<%= student.studentNationalID %>" readonly />
              <span class="input-group-addon"></span>
            </div>
            <div class="form-group input-group">
              <input type="text" name="studyCountry" class="form-control" id="studyCountry" value="<%= student.studyCountry %>" readonly />
              <span class="input-group-addon"></span>
            </div>
            <div class="form-group input-group">
              <input type="text" name="studyCountryGroup" class="form-control" id="studyCountryGroup" value="<%= student.studyCountryGroup %>" readonly />
              <span class="input-group-addon"></span>
            </div>
            <div class="input-group">
              <a class="btn btn-warning" href="/student/edit/<%= student.id %>"> Edit </a>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
